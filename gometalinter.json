{
  "Vendor": true,
  "Deadline": "5m",
  "Sort": ["linter", "severity", "path"],
  "Exclude": ["cli/compose/schema/bindata.go"],

  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],

  "Cyclo": 19,
  "LineLength": 200
}
